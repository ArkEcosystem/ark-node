{
  "name": "ark-node",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "doc": "docco -t docs/template/arkio.jst -c docs/template/arkio.css modules/* app.js README.md",
    "start:testnet": "node app.js --genesis genesisBlock.testnet.json --config config.testnet.json",
    "start:devnet": "node app.js --genesis genesisBlock.devnet.json --config config.devnet.json",
    "start:mainnet": "node app.js --genesis genesisBlock.mainnet.json --config config.mainnet.json -i",
    "start:test": "node app.js --genesis test/genesisBlock.json --config test/config.json",
    "test": "./node_modules/.bin/mocha",
    "cov": "./node_modules/.bin/mocha --require blanket -R html-cov test/helpers test/logic > tmp/coverage.html"
  },
  "author": "Boris Povod <boris@crypti.me>, Pavel Nekrasov <landgraf.paul@gmail.com>, Oliver Beddows <oliver@lisk.io>, FX Thoorens <fx.thoorens@ark.io>",
  "dependencies": {
    "arkjs": "github:arkecosystem/ark-js#mainnet",
    "async": "=2.6.1",
    "bip39": "^2.2.0",
    "body-parser": "=1.18.3",
    "bytebuffer": "=5.0.1",
    "change-case": "=3.0.2",
    "colors": "=1.3.2",
    "commander": "=2.19.0",
    "compression": "=1.7.3",
    "cors": "=2.8.4",
    "ejs": "=2.6.1",
    "express": "=4.16.3",
    "express-domain-middleware": "=0.1.0",
    "express-query-int": "=3.0.0",
    "express-rate-limit": "=3.2.1",
    "extend": "=3.0.2",
    "ip": "=1.1.5",
    "json-schema": "=0.2.3",
    "json-sql": "LiskHQ/json-sql#27e1ed1",
    "lodash": "=4.17.11",
    "method-override": "=3.0.0",
    "pg-monitor": "=1.0.0",
    "pg-native": "=3.0.0",
    "pg-promise": "=8.4.6",
    "popsicle": "=10.0.1",
    "pug": "^2.0.0-beta11",
    "randomstring": "=1.1.5",
    "rimraf": "=2.6.2",
    "socket.io": "=2.1.1",
    "strftime": "=0.10.0",
    "valid-url": "=1.0.9",
    "validator": "=10.8.0",
    "validator.js": "=2.0.3",
    "vorpal": "^1.11.4",
    "z-schema": "=3.24.1"
  },
  "devDependencies": {
    "bitcore-mnemonic": "=1.5.0",
    "blanket": "=1.2.3",
    "browserify-bignum": "=1.3.0-2",
    "buffer": "=5.2.1",
    "chai": "=4.2.0",
    "chai-bignumber": "=2.0.2",
    "crypto-browserify": "=3.12.0",
    "csv": "=4.0.0",
    "faker": "=4.1.0",
    "grunt": "=1.0.3",
    "grunt-cli": "=1.3.1",
    "grunt-contrib-compress": "=1.4.3",
    "grunt-contrib-jshint": "=2.0.0",
    "grunt-exec": "=3.0.0",
    "grunt-jsdox": "=0.1.7",
    "grunt-obfuscator": "=0.1.0",
    "jsdox": "=0.4.10",
    "jshint": "=2.9.6",
    "mocha": "=5.2.0",
    "moment": "=2.22.2",
    "supertest": "=3.3.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "helpers",
        "logic"
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "tmp"
      ]
    }
  }
}
